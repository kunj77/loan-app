rorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"98","messages":"99","suppressedMessages":"100","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/kunjanthakkar/Documents/Code/loan-app/src/index.tsx",[],[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/App.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/services/user.ts",["101"],[],"import { LoginData, User, Loan } from \"../typings\";\nimport http from \"./http-common\";\n\nclass UserDataService {\n  getAll() {\n    return http.get(\"/users\");\n  }\n\n  get(id: string) {\n    return http.get(`/users/${id}`);\n  }\n\n  create(data: User) {\n    return http.post(\"/users/create\", data);\n  }\n\n  login(data: LoginData) {\n    return http.post(\"/users/login\", data);\n  }\n\n  getLoans(userId: string) {\n    return http.get(`/users/${userId}/loans`);\n  }\n\n  getLoan(userId: string, loanId: string) {\n    return http.get(`/users/${userId}/loans/${loanId}`);\n  }\n\n  initiateLoan(id: string) {\n    return http.post(`/users/${id}/loans`);\n  }\n\n  submitLoan(id: string, data: {loanData: Loan, balanceSheet: any[]}, loanId: string) {\n    if(loanId === null) {\n      return http.post(`/users/${id}/loans`, data);\n    } else {\n      return http.post(`/users/${id}/loans/${loanId}`, data);\n    }\n  }\n}\n\nexport default new UserDataService();","/Users/kunjanthakkar/Documents/Code/loan-app/src/store/hooks.ts",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/store/userSlice.ts",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/store/store.ts",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/services/accountingProvider.ts",["102"],[],"import http from \"./http-common\";\n\nclass AccountingProviderService {\n  getAllProviders() {\n    return http.get(\"/accountingProvider/all\");\n  }\n  getBalanceSheet(companyTaxId: number) {\n    return http.get(`/accountingProvider/balance-sheet/${companyTaxId}`);\n  }\n}\n\nexport default new AccountingProviderService();","/Users/kunjanthakkar/Documents/Code/loan-app/src/components/ApplyLoanStepper.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/BusinessDetailsForm.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/ContentContainer.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/Home.tsx",["103","104","105"],[],"import React from 'react';\nimport { useNavigate } from 'react-router-dom';\nimport { useAppSelector, useAppDispatch } from '../store/hooks';\nimport UserDataService from '../services/user';\nimport { setLoans, setCurrentLoanId, setSelectedMenu } from '../store/userSlice';\nimport Approved from '../assets/approved.png';\nimport Rejected from '../assets/rejected.png';\nimport Pending from '../assets/pending.png';\nimport Edit from '../assets/edit.png';\nimport { AxiosError, AxiosResponse } from 'axios';\n\nconst Home = () => {\n  const navigate = useNavigate();\n  const dispatch = useAppDispatch();\n\n  const userData = useAppSelector((state) => state.user);\n  const loans = userData.loans;\n  const currentLoanId = userData.currentLoanId;\n\n  const editLoan = (loanId: string) => {\n    dispatch(setCurrentLoanId(loanId));\n    dispatch(setSelectedMenu(2));\n  }\n\n  const goToApplyLoan = () => {\n    if (currentLoanId === null) {\n      UserDataService.initiateLoan(userData.id as string)\n      .then((response: AxiosResponse<any>) => {\n        dispatch(setCurrentLoanId(response.data.id))\n      })\n      .catch((e: AxiosError) => {\n        console.log(\"failed to initiate loan\", e);\n      });\n    }\n\n    dispatch(setSelectedMenu(2));\n  }\n\n  React.useEffect(() => {\n    dispatch(setCurrentLoanId(null))\n\n    if (!userData.isLoggedIn) {\n      navigate('/login');\n    } else {\n      UserDataService.getLoans(userData.id as string)\n      .then((response: AxiosResponse) => {\n        dispatch(setLoans(response.data))\n      })\n      .catch((e: AxiosError) => {\n        console.log(\"Could not fetch loans\")\n      });\n    }\n  }, []);\n\n  return (\n    <div>\n      {loans.length > 0 ?\n        loans.map((loan) => (\n          <div key={loan._id} className=\"snapshot\">\n            <p className=\"title\">{loan.companyName || 'Untitled'}\n            {loan.status === 'Draft' && <img src={Edit} onClick={() => editLoan(loan._id)}/>}\n            </p>\n            <div className=\"content\">\n              <p><span className='label'>Amount: </span>{loan.loanAmount}</p>\n              <img src={loan.status === 'Approved' ? Approved : loan.status === 'Rejected' ? Rejected : Pending} />\n            </div>\n            <p className='footer'><span className='label'>Status: </span>{loan.status}</p>\n          </div>\n        )) :\n        <p>No loans applied yet! Get started <span className='clickable' onClick={goToApplyLoan}>here!</span></p>}\n    </div>\n  );\n};\n\nexport default Home;\n","/Users/kunjanthakkar/Documents/Code/loan-app/src/components/LoginForm.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/ReviewBalancesheet.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/MainContainer.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/Sidebar.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/SignupForm.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/services/http-common.ts",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/ReviewBalanceSheet.tsx",[],[],"/Users/kunjanthakkar/Documents/Code/loan-app/src/components/LoadingSpinner.tsx",[],[],{"ruleId":"106","severity":1,"message":"107","line":42,"column":1,"nodeType":"108","endLine":42,"endColumn":38},{"ruleId":"106","severity":1,"message":"107","line":12,"column":1,"nodeType":"108","endLine":12,"endColumn":48},{"ruleId":"109","severity":1,"message":"110","line":53,"column":6,"nodeType":"111","endLine":53,"endColumn":8,"suggesti